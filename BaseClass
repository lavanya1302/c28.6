class BaseClass(
    constructer(x,y,width,height,){
    var options={
        'restitution':0.8,
        'friction':1.0,
        'density':1.0
    }
    this.body=Bodies.circle(x,y,width,height,options);
    this.width=width;
    this.height=height;
    this.image=loadImage("sprites/base.png");
    World.add(world,this.body);
    }
    display(){
        
        push();
        imageMode(CENTER);
    }

    
}